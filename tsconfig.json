{
  "compilerOptions": {
    "lib": ["dom", "es2022"],
    "target": "es2022",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "importHelpers": true,
    "sourceMap": true,
    "jsx": "react"
  }
}
