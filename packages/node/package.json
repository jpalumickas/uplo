{
  "name": "@uplo/node",
  "version": "0.18.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "jpalumickas/uplo",
  "homepage": "https://uplo.js.org",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:dev": "vitest dev"
  },
  "dependencies": {
    "@uplo/types": "workspace:^",
    "@uplo/utils": "workspace:^",
    "camelcase": "^6.3.0",
    "deepmerge": "^4.3.1",
    "jose": "^4.14.6",
    "mime": "^3.0.0",
    "tempy": "^1.0.1"
  },
  "devDependencies": {
    "@types/mime": "^3.0.1",
    "@types/node": "^20.5.9",
    "vitest": "^0.34.3"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "express",
    "fastify",
    "gcs",
    "s3",
    "bucket",
    "upload",
    "uploads",
    "direct upload"
  ]
}
