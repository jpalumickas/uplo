{
  "name": "@uplo/server",
  "version": "0.19.4",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "jpalumickas/uplo",
  "homepage": "https://uplo.js.org",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:dev": "vitest dev"
  },
  "dependencies": {
    "@uplo/types": "workspace:^",
    "@uplo/utils": "workspace:^",
    "camelcase": "^6.3.0",
    "jose": "^5.9.2",
    "mime": "^3.0.0"
  },
  "devDependencies": {
    "@types/mime": "^3.0.4",
    "@types/node": "^20.12.2",
    "prettier": "^3.3.3",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "express",
    "fastify",
    "gcs",
    "s3",
    "bucket",
    "upload",
    "uploads",
    "direct upload"
  ]
}
