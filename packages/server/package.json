{
  "name": "@uplo/server",
  "version": "0.21.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./route-handler": {
      "import": {
        "types": "./dist/route-handler/index.d.ts",
        "default": "./dist/route-handler/index.js"
      },
      "require": {
        "types": "./dist/route-handler/index.d.cts",
        "default": "./dist/route-handler/index.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "jpalumickas/uplo",
  "homepage": "https://uplo.js.org",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest dev"
  },
  "dependencies": {
    "@uplo/types": "workspace:^",
    "@uplo/utils": "workspace:^",
    "camelcase": "^6.3.0",
    "hono": "^4.8.0",
    "jose": "^5.10.0",
    "mime": "^3.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/mime": "^3.0.4",
    "@types/node": "^22.15.32"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "express",
    "fastify",
    "gcs",
    "s3",
    "bucket",
    "upload",
    "uploads",
    "direct upload"
  ]
}
